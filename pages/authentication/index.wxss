.upload button {
  font-size: 26rpx;
  background-color: #2387fc;
}

/* 拍摄要求 */

.require {
  width: 750rpx;
  height: 400rpx;
  margin-top: 20rpx;
  padding: 30rpx 30rpx;
  box-sizing: border-box;
  background-color: #fff;
}

.require .require_m {
  margin-top: 20rpx;
  height: 68rpx;
  font-size: 26rpx;
  color: #333;
}

.require_m .font {
  color: red;
}

/* 图片对比规则 */

.require_rule {
  height: 246rpx;
}

.require_rule .photo {
  width: 160rpx;
  height: 96rpx;
  float: left;
  text-align: center;
  position: relative;
  margin: 60rpx 5rpx 0 5rpx;
  border: 1rpx dashed #ccc;
}

.photo image {
  width: 100%;
  height: 100%;
}

.photo .photo_label {
  width: 44rpx;
  height: 44rpx;
  border: 1rpx dashed #ccc;
  position: absolute;
  bottom: -22rpx;
  left: 60rpx;
}

.photo .label {
  margin-top: 20rpx;
  font-size: 24rpx;
  color: #919191;
}

.next {
  width: 690rpx;
  height: 88rpx;
  font-size: 34rpx;
  color: #fff;
  position: absolute;
  left: 30rpx;
  bottom: 73rpx;
  background-color: #2387fc;
}
