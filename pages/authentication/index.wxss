page {
  width: 750rpx;
  height: 1448rpx;
  position: relative;
  background-color: #f5f5f5;
}

.header {
  width: 750rpx;
  height: 200rpx;
}

.header image {
  width: 100%;
  height: 100%;
}

/* 拍摄上传 */

.upload {
  width: 750rpx;
  height: 400rpx;
  padding: 32rpx 30rpx;
  background-color: #fff;
}

.text {
  font-size: 32rpx;
  color: #868686;
}

.upload_image {
  width: 750rpx;
  height: 400rpx;
  margin-top: 31rpx;
}

.upload .left {
  width: 324rpx;
  height: 279rpx;
  margin-right: 30rpx;
  float: left;
  background-color: rgba(35, 136, 252, 0.144);
}

.box {
  width: 224rpx;
  height: 164rpx;
  border: 1rpx dashed #ccc;
  margin: 27rpx 40rpx;
  /* background: url('../../static/authentication/positive.png') no-repeat; */
  background-size: 100% 100%;
}

